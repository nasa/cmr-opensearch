<?xml version="1.0" encoding="UTF-8" ?>
 <OpenSearchDescription
  xmlns="http://a9.com/-/spec/opensearch/1.1/"
  xmlns:geo="http://a9.com/-/opensearch/extensions/geo/1.0/"
  xmlns:params="http://a9.com/-/spec/opensearch/extensions/parameters/1.0/"
  xmlns:time="http://a9.com/-/opensearch/extensions/time/1.0/">
  <ShortName>Radarsat Imagery</ShortName>
  <LongName>Metadata and browse imagery are obtained from a search of the CEOCat-NEODF catalogues at CCMEO, Natural Resources Canada.</LongName>
  <Description>Search for Raw Scenes of Radarsat-1 and Radarsat-2 Imagery; Contact : Natural Resources Canada, Canada Centre For Mapping And Earth Observation(CCMEO), 560 Rochester St., Ottawa, Ontario, Canada, email : bsa@ccrs.nrcan.gc.ca</Description>
  <Contact>bsa@ccrs.nrcan.gc.ca</Contact>

  <Image height="64" width="64" type="image/png">http://www.powerhousemuseum.com/collection/database/opensearch/phmlogo.png</Image>
  <Image height="16" width="16" type="image/gif">http://www.nrcan.gc.ca/sites/all/themes/nrcan/images/sig-eng.gif</Image>

  <!-- with reference to the Url element below, the startIndex and count parameters are mutually exclusive from the startPage parameter - the startPage parameter cannot be used with the startIndex and count parameters -->
  <Url type="text/html" template="https://eodms-sgdot.nrcan-rncan.gc.ca/apps/cgi/opensearch/eodms_opensearch_r1.sh?q=<%= @dataset_id %>&amp;id={geo:id?}&amp;bbox={geo:box?}&amp;dtstart={time:start?}&amp;dtend={time:end?}&amp;pw={startPage?}&amp;startIndex={startIndex?}&amp;count={count?}<%-if @client_id_model.valid? && !@client_id_model.clientId.blank? -%>&amp;clientId=<%= @client_id_model.clientId %><% end %>">
    <params:Parameter name="dtstart" value="{time:start}" minimum="1"  minInclusive="<%= @begin %>" maxInclusive="<%= @end %>" title="Temporal Start" />
    <params:Parameter name="dtend" value="{time:end}" minimum="0"  minInclusive="<%= @begin %>" maxInclusive="<%= @end %>" maxPeriod="P14D" relativeTo="{time:start}" title="Temporal End" />
  </Url>

  <Url type="application/atom+xml" rel="results" template="https://eodms-sgdot.nrcan-rncan.gc.ca/apps/cgi/opensearch/eodms_opensearch_r1.sh?q=<%= @dataset_id %>&amp;id={geo:id?}&amp;bbox={geo:box?}&amp;dtstart={time:start?}&amp;dtend={time:end?}&amp;pw={startPage?}&amp;startIndex={startIndex?}&amp;count={count?}&amp;format=atom<%-if @client_id_model.valid? && !@client_id_model.clientId.blank? -%>&amp;clientId=<%= @client_id_model.clientId %><% end %>">
    <params:Parameter name="dtstart" value="{time:start}" minimum="1"  minInclusive="<%= @begin %>" maxInclusive="<%= @end %>" title="Temporal Start" />
    <params:Parameter name="dtend" value="{time:end}" minimum="0"  minInclusive="<%= @begin %>" maxInclusive="<%= @end %>" maxPeriod="P14D" relativeTo="{time:start}" title="Temporal End" />
  </Url>

  <!-- the following 2 examples apply to searching for Radarsat-1 raw scenes -->
  <Query role="example" searchTerms="Radarsat-1" time:start="2008-06-01T23:06:24Z" time:end="2008-06-08T23:03:03Z" geo:box="-82,43,-74,46" startPage="1" format="atom" />
  <Query role="example" searchTerms="CWIC_REG_Radarsat-1" time:start="2008-06-01T23:06:24Z" time:end="2008-06-08T23:03:03Z" geo:box="-82,43,-74,46" startPage="1" format="atom" />

  <!-- the following 2 examples apply to searching for Radarsat-2 raw scenes -->
  <Query role="example" searchTerms="Radarsat-2" time:start="2008-06-01T23:06:24Z" time:end="2008-08-08T23:03:03Z" geo:box="-82,43,-74,46" startPage="1" format="atom" />
  <Query role="example" searchTerms="CWIC_REG" time:start="2008-06-01T23:06:24Z" time:end="2008-08-08T23:03:03Z" geo:box="-82,43,-74,46" startPage="1" format="atom" />
  <Query role="example" searchTerms="Radarsat-2" geo:box="-82,43,-74,46" startIndex="1" count="10" format="atom" />
  <Query role="example" searchTerms="Radarsat-2" geo:id="imageid_159095_sceneid_2_recfac_GATN_acqdate_2011-10-10T12_22_43.305" format="atom" />

  <AdultContent>false</AdultContent>
  <Attribution>Obtained from CCMEO Earth Observation Catalogu</Attribution>
  <Developer>Natural Resources Canada</Developer>
  <InputEncoding>UTF-8</InputEncoding>
  <Language>en-us</Language>
  <OutputEncoding>UTF-8</OutputEncoding>
  <SyndicationRight>open</SyndicationRight>
  <Tags>Radarsat-1 Radarsat-2 Satellite Scenes Remote Sensing</Tags>
</OpenSearchDescription>